{{!-- Partie Adrien page film --}}
<div class="container-fluid onbackgroundAL">
    <H2 id="titreAL">Notre catalogue de <span class="colorOfTitleAL">films</span></H2>
    <div class="row al">

        {{#each results.film.results}}
        <div class="col-12 col-sm-12 col-md-4 col-lg-3 mt-5">
            <a href="/movieDetail/{{this.id}}">
                <img src="https://image.tmdb.org/t/p/original/{{this.poster_path}}" width="200" class="img-fluid poster"
                    alt="{{this.title}}">
                {{!-- <p class=" titleOfMovie">{{this.title}}</p> --}}
            </a>
        </div>
        {{/each}}
    </div>
</div>