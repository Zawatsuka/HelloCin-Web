 <div class="onbackgroundAL">
       <div class="container-fluid onbackground">
           <h2 class="centerText">Les  <span class="colorsOfTitle">films</span> et <span class="colorsOfTitle">series</span> de votre recherche </h2>
           <div class="row margOnFilm">
               {{#each searchResultsTV.results}}
               {{!-- {{#if this.media_type == "tv"}} --}}
               <div class="col-12 col-sm-12 col-md-4 col-lg-3 mt-5">
                   <a href="/movieDetail/{{this.id}}">
                       <img src="https://image.tmdb.org/t/p/original/{{this.poster_path}}" width="200"
                           class="img-fluid viewDescriptionLT poster" alt="{{this.title}}">
                   </a>
               </div>
               {{!-- {{/if}} --}}
               {{/each}}
           </div>
       </div>